                    <option>{{this}}</option>
                    </select>
                {{/each}}
                </div>
                <button type="submit">Submit</button>
            </form>
        </div>


        <div class="pull-prize">
            <button id="back-to-kanban" onclick="pullPrize()">PULL</span></a>
        <div class="points-section">
        
</div>
