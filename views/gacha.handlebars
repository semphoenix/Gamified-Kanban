<div class="container">
        <a href="/kanban"><button id="back-to-kanban">Back To Kanban</button></a>
        <div class="points-section">
            <p id="points-text">Points:</p>
            <span id="points-num">{{Points}}</span>
        </div>

        
       <div class="prizes">
            <p>Prizes</p>
            <form action="/submit" method="POST">
                <div class="prize-selection">
                {{#each Prizes}}
